    "js",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "web",
    "theme",
    "google"
  ],
  "homepage": "http://todc.github.com/todc-bootstrap/",
  "author": "Tim O'Donnell",
  "scripts": {
    "test": "grunt test"
